<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8">
    <children>
        <HBox alignment="CENTER" spacing="24.0"/>
        <GridPane hgap="24.0" vgap="4.0">
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="NEVER"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="200.0" prefWidth="200.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Navn:" GridPane.rowIndex="3">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label text="E-Post:" GridPane.rowIndex="4">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label text="Antall personer:" GridPane.rowIndex="5">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <TextField fx:id="nameTextField" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="emailTextField" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TextField>
                <Label text="Kapasitet" GridPane.rowIndex="2">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <DatePicker fx:id="datePicker" editable="false" showWeekNumbers="true" GridPane.columnIndex="1"
                            GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </DatePicker>
                <ChoiceBox fx:id="hutChoiceBox" prefWidth="150.0" GridPane.columnIndex="1"/>
                <Label text="Dato:" GridPane.rowIndex="1">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label text="Koie">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Text fx:id="capacityText" strokeType="OUTSIDE" strokeWidth="0.0" text="# av totalt # plasser ledige."
                      GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Text>
                <ChoiceBox fx:id="countChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
            </children>
            <VBox.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </VBox.margin>
        </GridPane>
        <HBox alignment="CENTER_RIGHT" spacing="16.0">
            <children>
                <Button fx:id="commitButton" mnemonicParsing="false" text="Reserver"/>
                <Button fx:id="abortButton" mnemonicParsing="false" text="Avbryt"/>
            </children>
            <VBox.margin>
                <Insets bottom="8.0" top="8.0"/>
            </VBox.margin>
            <padding>
                <Insets right="8.0"/>
            </padding>
        </HBox>
    </children>
</VBox>
