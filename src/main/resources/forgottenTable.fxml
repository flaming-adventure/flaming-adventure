<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane minHeight="0.0" minWidth="0.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="no.flaming_adventure.controller.ForgottenTableController">
   <children>
      <VBox layoutX="8.0" layoutY="8.0" spacing="8.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox spacing="8.0">
               <children>
                  <Label text="Filter:" />
                  <DatePicker fx:id="filterFromDatePicker" />
                  <DatePicker fx:id="filterToDatePicker" />
               </children>
            </HBox>
            <Separator />
            <TableView fx:id="tableView" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <columns>
                  <TableColumn fx:id="hutColumn" editable="false" prefWidth="150.0" text="Koie" />
                  <TableColumn fx:id="itemColumn" editable="false" prefWidth="150.0" text="Ting" />
                  <TableColumn fx:id="commentColumn" editable="false" prefWidth="400.0" text="Kommentar" />
                  <TableColumn fx:id="nameColumn" editable="false" prefWidth="150.0" text="Navn" />
                  <TableColumn fx:id="emailColumn" editable="false" prefWidth="200.0" text="E-Post" />
                  <TableColumn fx:id="dateColumn" editable="false" prefWidth="75.0" text="Dato" />
               </columns>
            </TableView>
            <HBox>
               <children>
                  <Label text="Legg til" textFill="#888888" />
                  <Separator HBox.hgrow="ALWAYS">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <HBox.margin>
                        <Insets left="8.0" top="8.0" />
                     </HBox.margin>
                  </Separator>
               </children>
            </HBox>
            <HBox spacing="8.0">
               <children>
                  <ComboBox fx:id="hutComboBox" prefWidth="150.0" />
                  <DatePicker fx:id="datePicker" />
                  <ChoiceBox fx:id="reservationChoiceBox" prefWidth="300.0" />
                  <TextField fx:id="itemTextField" promptText="Ting" HBox.hgrow="ALWAYS" />
                  <Button fx:id="commitButton" mnemonicParsing="false" text="Lagre" />
               </children>
            </HBox>
            <TextField fx:id="commentTextField" promptText="Kommentar" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</AnchorPane>
