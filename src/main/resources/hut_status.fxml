<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox xmlns:fx="http://javafx.com/fxml/1" spacing="8.0" xmlns="http://javafx.com/javafx/8">
    <children>
        <HBox spacing="8.0">
            <children>
                <ChoiceBox fx:id="hutChoiceBox" prefWidth="150.0"/>
                <Text fx:id="firewoodText" strokeType="OUTSIDE" strokeWidth="0.0" text="Ved: # sekker.">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Text>
            </children>
        </HBox>
        <HBox alignment="TOP_CENTER" spacing="8.0">
            <children>
                <TitledPane collapsible="false" text="Gjenglemt">
                    <content>
                        <TableView fx:id="forgottenTableView" GridPane.columnIndex="1">
                            <columns>
                                <TableColumn fx:id="forgottenItemColumn" editable="false" prefWidth="150.0"
                                             text="Ting"/>
                                <TableColumn fx:id="forgottenCommentColumn" editable="false" prefWidth="400.0"
                                             text="Kommentar"/>
                                <TableColumn fx:id="forgottenNameColumn" editable="false" prefWidth="150.0"
                                             text="Navn"/>
                                <TableColumn fx:id="forgottenEmailColumn" editable="false" prefWidth="200.0"
                                             text="E-Post"/>
                                <TableColumn fx:id="forgottenDateColumn" editable="false" prefWidth="75.0" text="Dato"/>
                            </columns>
                        </TableView>
                    </content>
                </TitledPane>
            </children>
        </HBox>
        <HBox alignment="TOP_CENTER" spacing="8.0">
            <children>
                <TitledPane collapsible="false" text="Utstyr">
                    <content>
                        <TableView fx:id="equipmentTableView" GridPane.rowIndex="1">
                            <columns>
                                <TableColumn fx:id="equipmentItemColumn" prefWidth="150.0" text="Ting"/>
                                <TableColumn fx:id="equipmentCountColumn" prefWidth="75.0" text="Antall"/>
                                <TableColumn fx:id="equipmentDateColumn" prefWidth="75.0" text="Innkjøpt"/>
                            </columns>
                        </TableView>
                    </content>
                </TitledPane>
                <TitledPane collapsible="false" text="Ødelagt">
                    <content>
                        <TableView fx:id="destroyedTableView" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <columns>
                                <TableColumn fx:id="destroyedItemColumn" prefWidth="300.0" text="Ting"/>
                            </columns>
                        </TableView>
                    </content>
                </TitledPane>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
</VBox>
